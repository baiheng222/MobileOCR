<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="48dp"
    android:id="@+id/wb_acbar_photoId"
    android:background="@color/wboard_folder_manage_color"
    android:orientation="horizontal">
    <LinearLayout 
        android:id="@+id/left"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
		android:layout_centerVertical="true"
		android:gravity="center"
		android:visibility="invisible">
        <ImageView
	        android:id="@+id/bc_left_img_back"
	        android:layout_width="25dp"
	        android:layout_height="match_parent"
	        android:src="@mipmap/back"
	        android:layout_marginLeft="18dp"
	        android:layout_marginStart="18dp"
	        android:layout_gravity="center"
	        
        />
         <TextView 
            android:id="@+id/bc_left_title"
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:text="照片"
	        android:textSize="17sp"
	        android:textColor="@color/white"
	        android:layout_gravity="center"
	        android:gravity="center"/>
    </LinearLayout>
    
    <TextView 
        android:id="@+id/bc_center_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="相机胶卷"
        android:textSize="18sp"
        android:textColor="@color/white"
        android:layout_centerInParent="true"/>
    <TextView 
        android:id="@+id/bc_right_cancle"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="@string/bc_str_cancle"
        android:textSize="17sp"
        android:textColor="@color/white"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="18dp"
        android:layout_marginEnd="18dp"
        android:layout_centerVertical="true"
        android:gravity="center"
        />
</RelativeLayout>